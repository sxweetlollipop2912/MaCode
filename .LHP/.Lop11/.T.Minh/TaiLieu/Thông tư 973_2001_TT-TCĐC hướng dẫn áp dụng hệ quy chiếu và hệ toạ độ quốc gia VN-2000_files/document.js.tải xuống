$(document).ready(function(){
    $(document).pngFix();
});

function LoadScanDocument(imageUrl, pageCount, ticket) {
    LS_Reader.makeDraggable(getObj('divImg'), getObj('divImg'), getObj('divScroll'));
    try {
        LS_Reader.ImagePath = imageUrl;
        LS_Reader.Ticket = ticket;
        LS_Reader.PageCount = pageCount;
        LS_Reader.PageIndex = 0;
    } catch (e) { }
    LS_Reader.generateImg(LS_Reader.PageCount);
    genPageList(LS_Reader.indexObject, LS_Reader.PageCount, LS_Reader.PageIndex);
    LS_Reader.gotoPage(LS_Reader.PageIndex); LS_Reader.zoomChange();
}