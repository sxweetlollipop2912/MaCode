function AdvancedSearch() {
//    if ($.cookies.test()) {
//        $.cookies.set("Cookie_VB", "close");
//    }
    if ($("#txtKeyWord").val() == "< Tìm Văn bản Pháp luật >")
        $("#txtKeyWord").val("");
    var sTpe = "";
//    if ($("#cbSearchType") != 'undefined') {
//        if ($('#cbSearchType').val() != "0") {
//            sTpe = "&type=" + $('#cbSearchType').val();
//        } 
//    }
    window.location = "/phap-luat/tim-van-ban.aspx?keyword=" + encodeURIComponent($("#txtKeyWord").val()) + sTpe + "&match=True&area=0";
    return false;
}
function DTAdvancedSearch() {
//    if ($.cookies.test()) {
//        $.cookies.set("Cookie_VB", "close");
//    }
    if ($("#txtDTKeyWord").val() == "< Tìm Dự thảo Pháp luật >")
        $("#txtDTKeyWord").val("");
    var sTpe = "";

    window.location = "/phap-luat/tim-van-ban.aspx?keyword=" + encodeURIComponent($("#txtDTKeyWord").val()) + sTpe + "&match=True&area=0&status=100";
    return false;
}

function TTHCAdvancedSearch() {
    if ($("#txtTTHCKeyWord").val() == "< Tra cứu bản án >")
        $("#txtTTHCKeyWord").val("");
    window.location = "http://banan.thuvienphapluat.vn/tim-ban-an?q=" + $("#txtTTHCKeyWord").val();
    return false;
}
function CVAdvancedSearch() {
//    if ($.cookies.test()) {
//        $.cookies.set("Cookie_VB", "close");
//    }          
    if ($("#txtCVKeyWord").val() == "< Tìm Công văn >")
        $("#txtCVKeyWord").val("");
    window.location = "/phap-luat/tim-van-ban.aspx?keyword=" + encodeURIComponent($("#txtCVKeyWord").val()) + "&type=3" + "&match=True&area=0";
    return false;
}
function TVPLAdvancedSearch() {
    if ($("#txtTVPLKeyWord").val() == "<Tìm Thuật ngữ pháp lý >")
        $("#txtTVPLKeyWord").val("");
    window.open("https://thuvienphapluat.vn/tnpl/" + $("#txtTVPLKeyWord").val());
    return false;
}
function TinTucAdvancedSearch() {
    if ($("#txtTinTucKeyWord").val() == "< Tìm Tiêu Chuẩn Việt Nam >")
        $("#txtTinTucKeyWord").val("");
    window.location = "/phap-luat/tim-van-ban.aspx?keyword=" + encodeURIComponent($("#txtTinTucKeyWord").val()) + "&match=True&area=0&type=39";
    return false;
}

$(document).ready(function () {

    $(".idTabs-Home-Mid #txtKeyWord").blur(function () {
        if ($(".idTabs-Home-Mid #txtKeyWord").val() == "< Tìm Văn bản Pháp luật >") {
            $(".idTabs-Home-Mid #txtKeyWord").removeClass("txt-search-Home1");
            $(".idTabs-Home-Mid #txtKeyWord").addClass("txt-search-Home");
        }
    });

    $(".idTabs-Home-Mid #txtKeyWord").focus(function () {
        $(".idTabs-Home-Mid #txtKeyWord").removeClass("txt-search-Home");
        $(".idTabs-Home-Mid #txtKeyWord").addClass("txt-search-Home1");
    });

    $(".idTabs-Home-Mid #txtCVKeyWord").blur(function () {
        if ($(".idTabs-Home-Mid #txtCVKeyWord").val() == "< Tìm Công văn >") {
            $(".idTabs-Home-Mid #txtCVKeyWord").removeClass("txt-search-Home1");
            $(".idTabs-Home-Mid #txtCVKeyWord").addClass("txt-search-Home");
        }
    });

    $(".idTabs-Home-Mid #txtCVKeyWord").focus(function () {
        $(".idTabs-Home-Mid #txtCVKeyWord").removeClass("txt-search-Home");
        $(".idTabs-Home-Mid #txtCVKeyWord").addClass("txt-search-Home1");
    });

    $(".idTabs-Home-Mid #txtTTHCKeyWord").blur(function () {
        if ($(".idTabs-Home-Mid #txtTTHCKeyWord").val() == "< Tra cứu Xử phạt vi phạm hành chính >") {
            $(".idTabs-Home-Mid #txtTTHCKeyWord").removeClass("txt-search-Home1");
            $(".idTabs-Home-Mid #txtTTHCKeyWord").addClass("txt-search-Home");
