        $(".idTabs-New-Mid #txtCVKeyWord").removeClass("txt-search");
        $(".idTabs-New-Mid #txtCVKeyWord").addClass("txt-search1");
    });


    $('#btnTTHCKeyWord').click(function () {
        TTHCAdvancedSearch();
    });

    $('#btnTVPLKeyWord').click(function () {
        TVPLAdvancedSearch();
    });
    $('#btnKeyWordDetail').click(function () {
//        if ($.cookies.test()) {
//            $.cookies.set("Cookie_VB", "close");
//        }
        if ($("#txtKeyWord").val() == "< Tìm Văn bản Pháp luật >")
            $("#txtKeyWord").val("");
        var sTpe = "";

        if ($('#cbSearchType').val() != "0") {
            sTpe = "&type=" + $('#cbSearchType').val();
        }
        if ($('#cbSearchType').val() == "100") {
            sTpe = "&status=" + $('#cbSearchType').val();
        }
        window.location = "/phap-luat/tim-van-ban.aspx?keyword=" + encodeURIComponent($("#txtKeyWord").val()) + sTpe + "&match=True&area=0";
        return false;
    });
    $('#btnTinTucKeyWord').click(function () {
        TinTucAdvancedSearch();
    });

    $('#btnKeyWordHome').click(function () {
        AdvancedSearch();
    });
    $('#btnDTKeyWord').click(function () {
        DTAdvancedSearch();
    });

    $('#btnCVKeyWord').click(function () {
        CVAdvancedSearch();
    });


    $('#txtKeyWord').keypress(function (event) {
        if (event.keyCode == 13) {
            AdvancedSearch();
            return false;
        }
    });
    $('#txtDTKeyWord').keypress(function (event) {
        if (event.keyCode == 13) {
            DTAdvancedSearch();
            return false;
        }
    });
    $('#txtCVKeyWord').keypress(function (event) {
        if (event.keyCode == 13) {
            CVAdvancedSearch();
            return false;
        }
    });

    $('#txtTTHCKeyWord').keypress(function (event) {
        if (event.keyCode == 13) {
            TTHCAdvancedSearch();
            return false;
        }
    });

    $('#txtTVPLKeyWord').keypress(function (event) {
        if (event.keyCode == 13) {
            TVPLAdvancedSearch();
            return false;
        }
    });

    $('#txtTinTucKeyWord').keypress(function (event) {
        if (event.keyCode == 13) {
            TinTucAdvancedSearch();
            return false;
        }
    });

});