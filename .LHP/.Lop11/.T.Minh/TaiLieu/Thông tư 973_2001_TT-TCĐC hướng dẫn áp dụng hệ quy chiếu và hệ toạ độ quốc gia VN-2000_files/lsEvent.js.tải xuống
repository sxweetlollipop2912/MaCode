function lsEventPutCk(val) {
    if ($.cookies.test()) {
      
        var d = new Date();
        var today = new Date(d.getFullYear(), d.getMonth() + 1, d.getDate());
        var name = d.getFullYear() + "-" + (d.getMonth() + 1) + "-" + d.getDate();
        if ($.cookies.get(name) != null && $.cookies.get(name) != "null") {
            val = $.cookies.get(name) + "," + val;
        }
        $.cookies.set(name, val, { expiresAt: new Date(d.getFullYear(), d.getMonth(), d.getDate() + 1) });
     
    }
}

